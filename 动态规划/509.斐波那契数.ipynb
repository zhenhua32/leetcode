    "class Solution:\n",
    "    def fib(self, n: int) -> int:\n",
    "        if n == 0:\n",
    "            return 0\n",
    "        if n == 1:\n",
    "            return 1\n",
    "        a = 0\n",
    "        b = 1\n",
    "        for _ in range(2, n+1):\n",
    "            a, b = b, a+b\n",
    "        return b"
   ]
  }
